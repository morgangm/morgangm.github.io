mk_fieldmap <- function(
n_row = 23,
n_range = 30
){
x = n_range
y = n_row
f_map <- matrix(seq(1, x*y), nrow = x, ncol = y)
# every other row
eor <- seq(2, n_row, 2)
f_map[, eor] <- f_map[n_range:1, eor]
f_map <- data.frame(f_map)
f_map$V1 <- seq(1, n_range)
f_map <- f_map |>
pivot_longer(cols = starts_with('X'),
names_to = 'V2',
values_to = 'PlotID') |>
mutate(V2 = as.integer(str_remove_all(V2, 'X'))) |>
arrange(V2, V1)
f_map <- f_map[, c('V2', 'V1', 'PlotID')] |>
rename(Row = V1, Range = V2)
return(f_map)
}
